{
  "name": "discord-ban-sync",
  "author": "Almeida <almeidx@pm.me> (almeidx.dev)",
  "packageManager": "yarn@3.2.1",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "rimraf dist && tsc",
    "lint": "prettier --check . && eslint src --ext ts",
    "format": "prettier --write . && eslint src --ext ts --fix",
    "start": "yarn build && node --enable-source-maps dist/index.js",
    "update": "yarn upgrade-interactive"
  },
  "dependencies": {
    "discord.js": "14.0.0-dev.1657411900-f0b68d5",
    "dotenv": "^16.0.1",
    "pino": "^8.1.0",
    "pino-pretty": "^8.1.0"
  },
  "devDependencies": {
    "@types/node": "^18.0.3",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "eslint": "^8.19.0",
    "eslint-config-marine": "^9.4.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.7.4"
  }
}
